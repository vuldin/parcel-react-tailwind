{
  "name": "parcel-react-tailwind",
  "private": true,
  "scripts": {
    "watch": "parcel -p 8080 watch index.html",
    "start": "parcel -p 8080 index.html",
    "build": "parcel build index.html",
    "prettier": "prettier --write --loglevel error src/**/*.js",
    "lint": "eslint src",
    "clean": "rimraf dist/* .cache/*",
    "cypress:open": "cypress open",
    "test": "cypress run"
  },
  "keywords": [
    "parcel",
    "react",
    "tailwind",
    "prettier",
    "eslint"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.10",
    "@fortawesome/free-solid-svg-icons": "^5.6.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "alert": "^1.0.0",
    "autoprefixer": "^9.4.2",
    "postcss-modules": "^1.4.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "tailwindcss": "^0.6.6",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cypress": "^3.8.1",
    "cypress-testing-library": "^2.3.4",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.1.3",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.11.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2"
  }
}
